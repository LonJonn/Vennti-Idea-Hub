{
	"defaultSeverity": "warning",
	"extends": ["tslint:recommended", "tslint-config-prettier"],
	"linterOptions": {
		"exclude": ["node_modules/**"]
	},
	"rules": {
		"interface-name": false,
		"arrow-parens": false,
		"curly": false,
		"no-console": false,
		"member-access": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false
	}
}
